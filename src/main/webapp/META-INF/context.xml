<?xml version="1.0" encoding="UTF-8"?>

<Context antiJARLocking="true" path="honorWall">
	<Realm className="org.apache.catalina.realm.JDBCRealm" 
            debug="9"
            driverName="org.postgresql.Driver"
            connectionURL="jdbc:postgresql://localhost:5432/honorwall"
            connectionName="postgres"
            connectionPassword="postgres"
            userTable="usuario"
            userNameCol="ds_login"
            userCredCol="ds_senha"
            userRoleTable="papel_usuario"
            roleNameCol="ds_papel"
        />
</Context>